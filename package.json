  "description": "",
  "main": "app/app.js",
  "scripts": {
    "start": "browserify app/app.js -d > dist/app.js",
    "test": "BROWSERIFYSHIM_DIAGNOSTICS=1 browserify app/app.js -d > dist/app.js"
  },
  "author": "ajaegle",
  "license": "ISC",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
